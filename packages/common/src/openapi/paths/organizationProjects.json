{
    "get": {
        "summary": "List organization projects",
        "tags": ["organization", "project"],
        "operationId": "getOrganizationProjects",
        "parameters": [],
        "requestBody": {
            "content": {
                "application/json": {
                    "schema": {
                        "type": "object",
                        "properties": {
                            "role": {
                                "type": "string"
                            }
                        },
                        "additionalProperties": false
                    }
                }
            }
        },
        "responses": {
            "200": {
                "description": "Successfully got projects",
                "content": {
                    "application/json": {
                        "schema": {
                            "allOf": [
                                {
                                    "$ref": "../schemas/Success.json"
                                },
                                {
                                    "properties": {
                                        "results": {
                                            "type": "array",
                                            "items": {
                                                "$ref": "../schemas/Projects.json"
                                            }
                                        }
                                    },
                                    "required": ["results"]
                                }
                            ]
                        }
                    }
                }
            },
            "default": {
                "$ref": "../responses/ErrorResponse.json"
            }
        }
    }
}
